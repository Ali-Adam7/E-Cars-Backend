{
  "name": "e-cars-backend",
  "version": "1.0.0",
  "description": "backend of E-Cars Ecommerce System",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc && cp -r src/keys dist/",
    "start": "pm2 start dist/server.js --attach",
    "dev": "nodemon src/server.ts",
    "test": "NODE_ENV=TEST npm run migrate && jest --runInBand",
    "prisma": "prisma generate --schema=src/third-party/prisma/schema.prisma",
    "migrate": "npx prisma migrate dev --name init --schema=src/third-party/prisma/schema.prisma"
  },
  "repository": {
    "type": "git",
    "url": "e-cars-backend"
  },
  "author": "Ali Adam",
  "license": "ISC",
  "dependencies": {
    "@babel/traverse": "^7.24.6",
    "@prisma/client": "^5.14.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "inversify": "^6.0.2",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nodemon": "^3.1.0",
    "prisma": "^5.14.0",
    "prisma-dbml-generator": "^0.12.0",
    "superagent": "^9.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
